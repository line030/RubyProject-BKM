<h1>Listing workout_sessions</h1>

<table>
  <tr>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @workout_sessions.each do |workout_session| %>
  <tr>
    <td><%= workout_session.date %></td>
    <td><%= link_to 'Show', workout_session %></td>
    <td><%= link_to 'Edit', edit_workout_session_path(workout_session) %></td>
    <td><%= link_to 'Destroy', workout_session, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

